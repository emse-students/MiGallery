{
	"name": "migallery",
	"private": true,
	"version": "1.1.0",
	"description": "MiGallery, by MiTV",
	"type": "module",
	"author": "DeMASKe",
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/emse-students/MiGallery.git"
	},
	"homepage": "https://github.com/emse-students/MiGallery#readme",
	"keywords": [
		"sveltekit",
		"svelte",
		"vite",
		"bun",
		"gallery"
	],
	"files": [
		"build/",
		"src/",
		"README.md"
	],
	"engines": {
		"node": ">=18.0.0",
		"bun": "*"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint src/lib --ext .ts,.tsx,.js,.jsx --cache && eslint src/routes --ext .ts,.tsx,.js,.jsx --cache",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --ignore-pattern 'scripts/**' --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"deps:install": "bun install",
		"deps:install:pnpm": "pnpm install",
		"deps:install:npm": "npm install",
		"ci:install": "bun install --production",
		"ci:build": "bun install --production && npm run build",
		"prebuild": "npm run deps:install || npm run deps:install:pnpm || npm run deps:install:npm || true",
		"start": "bun run dev",
		"package": "bun run build && bun ./scripts/pack-bun.js",
		"package:legacy": "npm run build && node -e \"require('fs').mkdirSync('build/artifacts',{ recursive: true })\" && npm pack --pack-destination build/artifacts",
		"version:show": "node -e \"console.log(require('./package.json').version)\"",
		"db:init": "node ./scripts/init-db.cjs",
		"db:backup": "node ./scripts/backup-db.cjs",
		"db:inspect": "node ./scripts/inspect-db.cjs",
		"db:migrate": "node ./scripts/migrate-db.cjs",
		"test:cleanup": "node ./scripts/cleanup-test-artifacts.cjs",
		"test": "node scripts/test-with-server.mjs",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:unit": "vitest run",
		"test:albums": "vitest run tests/albums.test.ts",
		"test:users": "vitest run tests/users.test.ts",
		"test:admin": "vitest run tests/admin-auth.test.ts",
		"test:people": "vitest run tests/people-photoscv.test.ts",
		"test:immich": "vitest run tests/immich-proxy.test.ts",
		"test:favorites": "vitest run tests/favorites-external.test.ts",
		"test:e2e": "vitest run tests/e2e-integration.test.ts",
		"generate:secret": "node ./scripts/generate_cookie_secret.cjs",
		"generate:auth-secret": "node ./scripts/generate-auth-secret.cjs",
		"cleanup:chunked-temp": "node ./scripts/cleanup-chunked-temp.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/adapter-node": "^5.5.3",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.2.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/form-data": "^2.5.2",
		"@typescript-eslint/eslint-plugin": "^8.56.0",
		"@typescript-eslint/parser": "^8.55.0",
		"@vitest/coverage-v8": "^4.0.18",
		"autoprefixer": "^10.4.24",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"husky": "^9.1.7",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"svelte": "^5.53.2",
		"svelte-check": "^4.4.3",
		"svelte-eslint-parser": "^1.4.1",
		"tailwindcss": "^4.1.17",
		"tar": "^7.5.9",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@auth/core": "^0.41.1",
		"@auth/sveltekit": "^1.11.1",
		"@eslint/js": "^9.39.1",
		"better-sqlite3": "^12.6.2",
		"bun-types": "^1.3.9",
		"dotenv": "^17.3.1",
		"ffmpeg-static": "^5.1.0",
		"ffprobe-static": "latest",
		"form-data": "^4.0.5",
		"jspdf": "^4.1.0",
		"jspdf-autotable": "^5.0.7",
		"lucide-svelte": "^0.563.0",
		"sharp": "^0.34.5",
		"streamsaver": "^2.0.6",
		"svelte-adapter-bun": "^1.0.1",
		"typescript-eslint": "^8.54.0",
		"undici": "^7.21.0",
		"vitest": "^4.0.10"
	},
	"overrides": {
		"cookie": "^0.7.0"
	}
}
